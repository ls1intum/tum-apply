{
  "ci": {
    "collect": {
      "url": [],
      "numberOfRuns": 3
    },
    "upload": {
      "target": "filesystem",
      "outputDir": ".lighthouseci"
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 0.80
          }
        ],
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.90
          }
        ],
        "categories:best-practices": [
          "warn",
          {
            "minScore": 0.85
          }
        ]
      }
    }
  }
}
