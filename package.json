{
  "name": "tum-apply",
  "version": "1.0.5",
  "private": true,
  "description": "Modern portal for streamlined, inclusive, and transparent doctoral applications at TUM",
  "license": "MIT",
  "author": {
    "name": "Stephan Krusche",
    "email": "krusche@tum.de",
    "url": "https://aet.cit.tum.de/krusche"
  },
  "scripts": {
    "prebuild": "node prebuild.mjs",
    "build": "npm run webapp:prod --",
    "clean-www": "rimraf build/resources/main/static/",
    "cleanup": "rimraf build/",
    "compile:ts": "npm run prebuild && tsc --project tsconfig.app.json",
    "compile:ts:tests": "npm run prebuild && tsc --project tsconfig.spec.json",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "prettier --check \"{,src/**/,webpack/,.blueprint/**/}*.{md,json,yml,js,cjs,mjs,ts,cts,mts,java,html,css,scss}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/,.blueprint/**/}*.{md,json,yml,js,cjs,mjs,ts,cts,mts,java,html,css,scss}\"",
    "serve": "npm run start --",
    "services:up": "docker compose -f src/main/docker/services.yml up --wait",
    "start": "npm run prebuild -- --develop && ng serve --hmr",
    "pretest": "npm run lint",
    "test": "vitest run --coverage",
    "test-diff:ci": "npm run prebuild && bash -lc 'set -eo pipefail; DEFAULT_BRANCH=${GITHUB_BASE_REF:-$(git remote show origin | sed -n \"/HEAD branch/s/.*: //p\")} ; git fetch -q origin \"$DEFAULT_BRANCH\"; CHANGED=$(git diff --name-only --diff-filter=ACMRTUXB \"origin/$DEFAULT_BRANCH\"...HEAD | grep -E \"^src/.*\\.(test|spec)\\.(ts|tsx)$\" || true); if [ -n \"$CHANGED\" ]; then echo \"Running changed tests:\"; echo \"$CHANGED\"; npx vitest run --coverage $CHANGED; else echo \"No changed spec files; running full suite\"; npx vitest run --coverage; fi'",
    "test:ci": "npm run prebuild && vitest run --coverage.enabled=true --coverage.thresholds.statements=85 --coverage.thresholds.branches=85 --coverage.thresholds.functions=85 --coverage.thresholds.lines=85",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "update": "ncu -i --format group",
    "webapp:build": "npm run prebuild -- --develop && ng build --configuration development",
    "webapp:dev": "npm run prebuild -- --develop && ng serve",
    "webapp:prod": "npm run prebuild && NG_BUILD_OPTIMIZE_CHUNKS=1 ng build --configuration production"
  },
  "config": {
    "default_environment": "prod",
    "packaging": "jar",
    "server_port": 8080
  },
